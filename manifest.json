{
  "name": "IAdB",
  "description": "igoist AdBlock, aim at baidu, ...",
  "version": "0.0.1",
  "background": {
    "scripts": ["src/background.js"]
  },
  "icons": {
    "32": "img/32.ico",
    "48": "img/icon.png",
    "128": "img/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.zhangxinxu.com/*"],
      "js": ["src/zhangxinxu.js"]
    },
    {
      "matches": ["*://*.baidu.com/*"],
      "js": ["src/baidu.js"]
    },
    {
      "matches": ["*://lodash.com/*"],
      "js": ["src/lodash.js"]
    },
    {
      "matches": ["*://*.csdn.net/*"],
      "js": ["src/csdn.js"]
    },
    {
      "matches": ["*://www.google.com.hk/*"],
      "js": ["src/google.js"]
    },
    {
      "matches": ["*://www.zhihu.com/*"],
      "js": ["src/zhihu.js"]
    }
  ],
  "manifest_version": 2,
  "options_page": "src/options.html",
  "page_action": {
    "default_icon": "img/128.png",
    "default_title": "自定义广告屏蔽扩展"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation", "*://*/*"
  ]
}
