{
  "name": "IAdB",
  "description": "igoist AdBlock, aim at baidu, ...",
  "version": "0.0.3",
  "background": {
    "scripts": ["dist/js/background.bundle.min.js"],
    "persistent": false
  },
  "icons": {
    "32": "img/32.ico",
    "48": "img/icon.png",
    "128": "img/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "run_at": "document_end",
      "js": ["dist/dll/react.dll.js", "dist/js/oneForAll.bundle.min.js"],
      "css": ["dist/css/IAdB.css"],
      "exclude_matches": ["http://127.0.0.1:333/*", "http://127.0.0.1:3333/*", "http://localhost:3333/*"]
    },
    {
      "matches": ["*://xxxxxxxxxxpinban.com/*"],
      "run_at": "document_end",
      "js": ["src/ptest.js"]
    },
    {
      "matches": ["*://*.zhangxinxu.com/*"],
      "js": ["src/zhangxinxu.js"]
    },
    {
      "matches": ["*://*.baidu.com/*"],
      "js": ["src/baidu.js"]
    },
    {
      "matches": ["*://lodash.com/*"],
      "js": ["src/lodash.js"]
    },
    {
      "matches": ["*://*.csdn.net/*"],
      "js": ["src/csdn.js"]
    },
    {
      "matches": ["*://www.google.com.hk/*", "*://www.google.com/*"],
      "js": ["src/google.js"]
    },
    {
      "matches": ["*://*.zhihu.com/*"],
      "js": ["dist/js/zhihu.bundle.min.js"]
    }
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "options_page": "pages/option/index.html",
  "chrome_url_overrides": {
    "newtab": "pages/newtab/index.html"
  },
  "browser_action": {
    "default_icon": "img/128.png",
    "default_title": "自定义广告屏蔽扩展",
    "default_popup": "pages/popup/index.html"
  },
  "permissions": ["activeTab", "tabs", "storage", "declarativeContent", "webNavigation", "*://*/*"],
  "web_accessible_resources": ["dist/css/pinit.min.css", "dist/css/IAdB.css", "dist/dll/reactForPinit.dll.js", "dist/js/pinit.min.js"]
}
